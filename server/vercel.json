{
  "version": 2,
  "builds": [
      {
          "src": "*.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "server/index.js",
          "methods": ["GET", "POST", "PUT", "PATCH", "OPTIONS"],
          "headers": {
            "Access-Control-Allow-Origin": "*"
          }
      }
  ]
}
