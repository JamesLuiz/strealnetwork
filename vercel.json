{
    "version": 2,
    "builds": [
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/server/(.*)",
        "dest": "server/index.js"
      },
      {
        "src": "/(?!server/)(.*)",
        "dest": "client/build/$1"
      }
    ]
  }